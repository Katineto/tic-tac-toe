     } else {\r\n                    winnerId = 2\r\n                }\r\n                console.log('We have a winner')\r\n                return true\r\n            } else {\r\n                switchPlayer()\r\n                return false\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\nmodule.exports = {\r\n    createBoard,\r\n    createGame,\r\n    createPlayer\r\n}"},"lineCount":null}},"hash":"50ff9c6409011cacf6e73274f681ddc8","cacheData":{"env":{}},"id":10}